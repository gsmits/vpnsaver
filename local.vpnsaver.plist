<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>local.vpnsaver</string>
	<key>ProgramArguments</key>
	<array>
		<string>sh</string>
		<string>-c</string>
		<string>~/Library/vpnsaver/vpnsaver.sh</string>
	</array>
    <key>StandardOutPath</key>
        <string>/Users/sporter/Library/Logs/vpnsaver.log</string>
    <key>StandardErrorPath</key>
        <string>/Users/sporter/Library/Logs/vpnsaver.log</string>
    <key>Debug</key>
        <true/>
    <key>StartInterval</key>
	<integer>180</integer>
</dict>
</plist>

